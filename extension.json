{
  "name": "MW_EXT_Align",
  "version": "1.0.0",
  "author": [
    "[https://z17.dev/ z17 Development]",
    "[https://z17.cx/ z17 CX]",
    "..."
  ],
  "url": "https://z17.dev/",
  "descriptionmsg": "mw-ext-align-desc",
  "license-name": "[https://choosealicense.com/licenses/mit/ MIT]",
  "type": "parserhook",
  "require": {
    "MediaWiki": ">= 1.31.0",
    "extensions": {
      "MW_EXT_Kernel": "*"
    }
  },
  "AutoloadClasses": {
    "MediaWiki\\Extension\\Z17\\MW_EXT_Align": "MW_EXT_Align.class.php"
  },
  "Hooks": {
    "ParserFirstCallInit": [
      "MediaWiki\\Extension\\Z17\\MW_EXT_Align::onParserFirstCallInit"
    ],
    "BeforePageDisplay": [
      "MediaWiki\\Extension\\Z17\\MW_EXT_Align::onBeforePageDisplay"
    ]
  },
  "MessagesDirs": {
    "MW_EXT_Align": [
      "i18n"
    ]
  },
  "ResourceModules": {
    "ext.mw.align.styles": {
      "styles": [
        "styles/theme.css"
      ],
      "position": "top",
      "targets": [
        "desktop",
        "mobile"
      ]
    }
  },
  "ResourceFileModulePaths": {
    "localBasePath": "modules",
    "remoteExtPath": "MW_EXT_Align/modules"
  },
  "manifest_version": 2
}
